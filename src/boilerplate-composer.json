{
  "name": "ideasonpurpose/iop-example-site",
  "description": "Block-based hybrid theme for the Example Site.",
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "vendor-dir": "wp-content/themes/THEME_NAME/vendor",
    "platform": {
      "php": "8.2",
      "ext-intl": "0"
    }
  },
  "authors": [
    {
      "name": "Ideas On Purpose",
      "homepage": "https://www.ideasonpurpose.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "IdeasOnPurpose\\": [
        "wp-content/themes/THEME_NAME/lib"
      ]
    }
  },
  "require": {
    "ideasonpurpose/wp-admin-separators": "1.0.8",
    "ideasonpurpose/wp-google-analytics": "^1.1.0",
    "ideasonpurpose/wp-svg-lib": "dev-master",
    "ideasonpurpose/wp-theme-init": "^2.13.2"
  }
}
